{  
    "plugins": [ "jasmine" ],
    "env": {
        "jasmine": true
    },
    "extends" : ["airbnb-base/legacy", "plugin:jasmine/recommended"],
    "rules": {
        "func-names": "off",
        "no-alert": "off",
        "no-console": "off",
        "no-plusplus": "off",
        "no-unused-vars": "warn",
        "vars-on-top": "off",
        "no-multi-spaces": "off",
        "key-spacing": "off",
        "max-len": "off",
        "no-undef": "off",
        "no-param-reassign": "off"
    },
    "eslint.options": { "configFile": "/Users/pierre/projects/ironhack/ironhack-labs/lab-javascript-memory-game/" }
}
